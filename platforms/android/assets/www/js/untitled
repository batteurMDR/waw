var wawapi = {

	root : "http://api.whereandwhere.com/v1",
	key : "",
	secret : "",
	token : "",

	call : function(method,url,body){
		var url = wawapi.root+url;
		if(typeof body === null){
			body = "";
		}else{
			body = JSON.stringify(body);
		}
		if(body != "" && wawapi.token != ""){
			console.log("1");
			console.log(body);
		}else if(body != ""){
			$.ajax({
				type: method,
			    url: url,
			    dataType : 'json',
			    beforeSend : function(xhr) {
			      	xhr.setRequestHeader("Authorization", wawapi.key+":"+wawapi.secret);
			      	xhr.setRequestHeader("Data", body);
			    },
			    error : function() {
			      	alert('error');
			    },
			    success: function(data) {
			        alert('success');console.log(data);
			    }
			});
		}else{
			$.ajax({
			    url: "http://google.com",
			    error : function() {
			      	alert('error');
			    },
			    success: function(data) {
			        alert('success');console.log(data);
			    }
			});
		}
	}

}